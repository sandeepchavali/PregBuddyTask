<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:customui="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.PregBuddyTask.activites.LoginActivity">


    <com.PregBuddyTask.customui.TextView_regular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        customui:aller_rg="aller_Rg.ttf" />


    <com.PregBuddyTask.customui.TextView_Bold
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World bold!"
        android:textSize="14sp"
        customui:aller_bd="aller_Bd.ttf"
        />

</android.support.constraint.ConstraintLayout>
